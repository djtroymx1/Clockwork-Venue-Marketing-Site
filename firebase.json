{
  "hosting": [
    {
      "site": "clockwork-console",
      "public": "web-ui",
      "ignore": ["**/.*", "**/node_modules/**"],
      "rewrites": [{ "source": "**", "destination": "/index.html" }],
      "headers": [
        { "source": "/index.html", "headers": [{ "key": "Cache-Control", "value": "no-store, max-age=0" }] },
        { "source": "/assets/**", "headers": [{ "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }] },
        { "source": "/**", "headers": [{ "key": "Strict-Transport-Security", "value": "max-age=31536000; includeSubDomains; preload" }] }
      ]
    },
    {
      "site": "clockworkvenue",
      "public": "sites/www/public",
      "ignore": ["**/.*", "**/node_modules/**"],
      "headers": [
        { "source": "/index.html", "headers": [{ "key": "Cache-Control", "value": "no-store" }] },
        { "source": "/service-worker.js", "headers": [{ "key": "Cache-Control", "value": "no-store" }] },
        { "source": "/**", "headers": [{ "key": "Strict-Transport-Security", "value": "max-age=31536000; includeSubDomains" }] }
      ],
      "rewrites": [
        { "source": "**", "destination": "/index.html" }
      ]
    },
    {
      "site": "clockworkvenueapex",
      "redirects": [
        { "source": "/**", "destination": "https://www.clockworkvenue.com", "type": 308 }
      ],
      "headers": [
        { "source": "/**", "headers": [{ "key": "Strict-Transport-Security", "value": "max-age=31536000; includeSubDomains" }] }
      ]
    },
    {
      "site": "stageflow-marketing-site",
      "public": "sites/legacy/public",
      "ignore": ["**/.*", "**/node_modules/**"],
      "rewrites": [
        { "source": "**", "destination": "/index.html" }
      ]
    }
  ]
}
